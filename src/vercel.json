{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --production=false",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "functions": {},
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}