{
  "buildCommand": "npx vite build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
